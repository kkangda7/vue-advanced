{"ast":null,"code":"import { computed } from '@vue/runtime-core';\nimport { useRoute } from 'vue-router';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n    const user = computed(() => {\n      return store.state.user;\n    });\n\n    const createApiUser = () => {\n      console.log(route);\n      const userName = route.params.id;\n      store.dispatch('FETCH_USER', userName);\n    };\n\n    createApiUser();\n    return {\n      user\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsBA,SAASA,QAAT,QAAyB,mBAAzB;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;AACXC,OAAK,GAAG;AACR,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AACA,UAAMI,KAAI,GAAIH,QAAQ,EAAtB;AACA,UAAMI,IAAG,GAAIN,QAAQ,CAAC,MAAK;AACzB,aAAOK,KAAK,CAACE,KAAN,CAAYD,IAAnB;AACD,KAFoB,CAArB;;AAGA,UAAME,aAAY,GAAI,MAAM;AAC1BC,aAAO,CAACC,GAAR,CAAYN,KAAZ;AACA,YAAMO,QAAO,GAAIP,KAAK,CAACQ,MAAN,CAAaC,EAA9B;AACAR,WAAK,CAACS,QAAN,CAAe,YAAf,EAA4BH,QAA5B;AACF,KAJA;;AAOAH,iBAAa;AAEb,WAAO;AACLF;AADK,KAAP;AAGF;;AAnBa,CAAf","names":["computed","useRoute","useStore","setup","route","store","user","state","createApiUser","console","log","userName","params","id","dispatch"],"sourceRoot":"","sources":["/Users/kanghoon/Desktop/vue-news/src/components/UserProfile.vue"],"sourcesContent":["<template>\n  <!-- <div>\n    <p>name: {{ user.id }}</p>\n    <p>karma: {{ user.karma }}</p>\n    <p>create: {{ user.created }}</p>\n  </div>   -->\n  <div class=\"user-container\">\n    <div>\n      <i class=\"fas fa-user\"></i>\n    </div>\n    <div class=\"user-description\">\n      <div>\n        <div>{{ user.id  }}</div>\n      </div>\n      <div class=\"time\">\n        {{ user.created }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed } from '@vue/runtime-core';\nimport { useRoute } from 'vue-router';\nimport { useStore } from 'vuex'\n\nexport default {\n    setup() {\n    const route = useRoute();\n    const store = useStore();\n    const user = computed(()=> {\n      return store.state.user\n    })\n    const createApiUser = () => {\n      console.log(route);\n      const userName = route.params.id\n      store.dispatch('FETCH_USER',userName)\n    }\n\n\n    createApiUser();\n\n    return {\n      user\n    }\n  }\n\n}\n</script>\n\n<style scoped>\n.user-container {\n  display: flex;\n  align-items: center;\n  padding: 0.5rem;\n}\n.fa-user {\n  font-size: 2.5rem;\n}\n.user-description {\n  padding-left: 8px;\n}\n.time {\n  font-size: 0.7rem;\n}\n</style>"]},"metadata":{},"sourceType":"module"}