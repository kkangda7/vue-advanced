e = useRoute();\n    const store = useStore();\n    const userInfo = computed(()=> {\n      return store.state.user\n    })\n    const createApiUser = () => {\n      const userName = route.params.id\n      store.dispatch('FETCH_USER',userName)\n    }\n\n\n    createApiUser();\n\n    return {\n      userInfo\n    }\n  }\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}