{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nexport default {\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n    const name = route.name; // eslint-disable-next-line vue/return-in-computed-property\n\n    const listItems = computed(() => {\n      if (name === 'news') {\n        return store.state.news;\n      } else if (name === 'ask') {\n        return store.state.asks;\n      } else if (name === 'jobs') {\n        return store.state.jobs;\n      }\n    });\n\n    const createApiNews = () => {\n      if (name === 'news') {\n        store.dispatch('FETCH_NEWS');\n      } else if (name === 'ask') {\n        store.dispatch('FETCH_ASK');\n      } else if (name === 'jobs') {\n        store.dispatch('FETCH_JOBS');\n      }\n    };\n\n    createApiNews();\n    return {\n      name,\n      listItems\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4CA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,QAAT,QAAyB,YAAzB;AAEA,eAAe;AACbC,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIF,QAAQ,EAAtB;AACA,UAAMG,KAAI,GAAIJ,QAAQ,EAAtB;AACA,UAAMK,IAAG,GAAIF,KAAK,CAACE,IAAnB,CAHM,CAKN;;AACA,UAAMC,SAAQ,GAAIP,QAAQ,CAAC,MAAM;AAC7B,UAAGM,IAAG,KAAM,MAAZ,EAAoB;AAClB,eAAOD,KAAK,CAACG,KAAN,CAAYC,IAAnB;AACF,OAFA,MAEO,IAAIH,IAAG,KAAM,KAAb,EAAoB;AACzB,eAAOD,KAAK,CAACG,KAAN,CAAYE,IAAnB;AACF,OAFO,MAEA,IAAIJ,IAAG,KAAM,MAAb,EAAqB;AAC1B,eAAOD,KAAK,CAACG,KAAN,CAAYG,IAAnB;AACF;AACH,KARyB,CAA1B;;AASA,UAAMC,aAAY,GAAI,MAAM;AAC1B,UAAGN,IAAG,KAAM,MAAZ,EAAoB;AAClBD,aAAK,CAACQ,QAAN,CAAe,YAAf;AACF,OAFA,MAEO,IAAGP,IAAG,KAAM,KAAZ,EAAmB;AACxBD,aAAK,CAACQ,QAAN,CAAe,WAAf;AACF,OAFO,MAEA,IAAGP,IAAG,KAAM,MAAZ,EAAoB;AACzBD,aAAK,CAACQ,QAAN,CAAe,YAAf;AACF;AACF,KARA;;AASAD,iBAAa;AAGb,WAAO;AACLN,UADK;AAELC;AAFK,KAAP;AAIF;;AAhCa,CAAf","names":["computed","useStore","useRoute","setup","route","store","name","listItems","state","news","asks","jobs","createApiNews","dispatch"],"sourceRoot":"","sources":["/Users/kanghoon/Desktop/vue-news/src/components/ListItem.vue"],"sourcesContent":["<template>\n  <div>\n    <ul class=\"news-list\">\n      <li \n        v-for=\"item in listItems\" \n        :key=\"item\" \n        class=\"post\"\n      >\n        <div class=\"points\">\n          {{ item.points || 0 }}\n        </div>\n        <div>\n          <p class=\"news-title\">\n            <template v-if=\"item.domain\">\n              <a :href=\"item.url\">\n                {{ item .title }}\n              </a>\n            </template>\n            <template v-else>\n              <router-link :to=\"`/item/${item.id}`\">\n                {{ item.title }}\n              </router-link>\n            </template>\n          </p>\n          <small class=\"link-text\">{{ item.time_ago }} by\n            <router-link \n              v-if=\"item.user\"\n              :to=\"`/user/${item.user}`\"\n              class=\"link-text\"\n            >{{ item.user }}\n            </router-link> \n            <a \n              v-else\n              :href=\"item.url\"\n            >{{ item.domain }}\n            </a> \n          </small>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport { computed } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRoute } from 'vue-router'\n\nexport default {\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n    const name = route.name\n   \n    // eslint-disable-next-line vue/return-in-computed-property\n    const listItems = computed(() => {\n        if(name === 'news') {\n          return store.state.news\n        } else if (name === 'ask') {\n          return store.state.asks\n        } else if (name === 'jobs') {\n          return store.state.jobs\n        }\n    })\n    const createApiNews = () => {\n      if(name === 'news') {\n        store.dispatch('FETCH_NEWS');\n      } else if(name === 'ask') {\n        store.dispatch('FETCH_ASK');\n      } else if(name === 'jobs') {\n        store.dispatch('FETCH_JOBS')\n      }\n    }\n    createApiNews();\n\n\n    return {\n      name,\n      listItems\n    }\n  }\n}\n</script>\n\n<style scoped>\n.news-list {\n  margin: 0;\n  padding: 0;\n}\n.post {\n  list-style: none;\n  display: flex;\n  align-items: center;\n  border-bottom: 1px solid #eee ;\n}\n.points {\n  width: 80px;\n  height: 60px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  color: #42b883;\n}\n.news-title {\n  margin: 0;\n}\n.link-text {\n  color: #828282;\n}\n</style>"]},"metadata":{},"sourceType":"module"}